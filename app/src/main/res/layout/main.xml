<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainView"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="162dp"
        android:layout_height="162dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="120dp"
        android:background="#3FFF01"
        android:cropToPadding="true"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/baseline_alarm_24" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="162dp"
        android:layout_height="162dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="88dp"
        android:background="#3FFF01"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/imageButton"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/baseline_music_note_24" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="162dp"
        android:layout_height="162dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="120dp"
        android:background="#3FFF01"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/baseline_games_24" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="162dp"
        android:layout_height="162dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="88dp"
        android:background="#3FFF01"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/imageButton10"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/baseline_settings_24" />

</androidx.constraintlayout.widget.ConstraintLayout>