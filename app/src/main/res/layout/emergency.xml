<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainView"
    android:layout_width="match_parent"
    android:layout_height="600dp">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="162dp"
        android:layout_height="162dp"
        android:background="#2AA92E"
        android:cropToPadding="true"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/baseline_alarm_24"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="268dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="162dp"
        android:layout_height="162dp"
        android:layout_marginBottom="88dp"
        android:background="#2AA92E"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/imageButton"
        app:srcCompat="@drawable/baseline_music_note_24"
        tools:layout_editor_absoluteX="16dp" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="162dp"
        android:layout_height="162dp"
        android:background="#2AA92E"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/baseline_games_24"
        tools:layout_editor_absoluteX="233dp"
        tools:layout_editor_absoluteY="268dp" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="162dp"
        android:layout_height="162dp"
        android:layout_marginBottom="88dp"
        android:background="#2AA92E"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/imageButton10"
        app:srcCompat="@drawable/baseline_settings_24"
        tools:layout_editor_absoluteX="233dp" />

</androidx.constraintlayout.widget.ConstraintLayout>